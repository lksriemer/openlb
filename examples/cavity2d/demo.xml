<Param>
  <Mesh>
    <lx>1</lx>
    <ly>1</ly>
    <Refinement>128</Refinement>
  </Mesh>
  <Solver>
    <Type>stationary</Type>
    <PressureFilter>True</PressureFilter>
    <Tolerance>1.e-10</Tolerance>
  </Solver>
  <VisualizationVTK>
    <Filename>cavity2dvtk</Filename>
    <SaveTime>1.</SaveTime>
    <Vector>Velocity</Vector>
    <Scalar>Pressure</Scalar>
  </VisualizationVTK>
  <VisualizationImages>
    <Filename>cavity2dimage</Filename>
    <SaveTime>1.</SaveTime>
    <Scalar>Pressure</Scalar>
  </VisualizationImages>
  <Log>
    <Filename>cavity2d.log</Filename>
    <SaveTime>0.1</SaveTime>
    <Scalar>Pressure</Scalar>
  </Log>
  <Application>
    <Name>cavity2d</Name>
    <OlbDir>../../</OlbDir>
    <OutputDir>./tmp/</OutputDir>
    <Dimension>3</Dimension>
    <MaxTime>20.</MaxTime>
    <MaxU>1.0e-02</MaxU>
    <Re>100.</Re>
  </Application>
</Param>
